extends ../layout

block content
  .contentBlock
    h3= title
      hr
      br
    table.table.assignListTable
      thead.thead-dark
        tr
          each item in list
            th(scope='col')= item
      tbody
        each i, idx in invest
            tr.investList(idNum=`${i.id}`)
              th(scope='row')= idx+1
              td= i.category
              td= i.asset_name
              td= i.user_id.name
              td= i.createdAt

